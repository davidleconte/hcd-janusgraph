  {
   "cell_type": "code",
   "execution_count": null,
   "id": "shortest-path",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Shortest path between two people (using NetworkX)\n",
    "# Convert to undirected for path finding\n",
    "G_undirected = G.to_undirected()\n",
    "\n",
    "# Get Alice and Eve's IDs\n",
    "alice_id = [n for n, d in G.nodes(data=True) if d.get('name') == 'Alice Johnson'][0]\n",
    "eve_id = [n for n, d in G.nodes(data=True) if d.get('name') == 'Eve Davis'][0]\n",
    "\n",
    "try:\n",
    "    path = nx.shortest_path(G_undirected, alice_id, eve_id)\n",
    "    path_names = [G.nodes[n].get('name', str(n)) for n in path]\n",
    "    print(f\"\\nüõ§Ô∏è  Shortest path from Alice to Eve:\")\n",
    "    print(f\"   {' ‚Üí '.join(path_names)}\")\n",
    "    print(f\"   Length: {len(path) - 1} hops\")\n",
    "except nx.NetworkXNoPath:\n",
    "    print(\"\\n‚ùå No path found between Alice and Eve\")"
   ]
  },
  {
   "cell_type": "code",
