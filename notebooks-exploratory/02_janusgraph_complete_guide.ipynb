   "source": [
    "# Find common connections (mutual friends)\n",
    "query = \"\"\"\n",
    "g.V().has('person', 'name', 'Alice Johnson').as('alice')\n",
    "  .out('knows').as('friend')\n",
    "  .V().has('person', 'name', 'Bob Smith')\n",
    "  .out('knows')\n",
    "  .where(eq('friend'))\n",
    "  .values('name')\n",
    "\"\"\"\n",
    "\n",
    "mutual = gc.submit(query).all().result()\n",
    "print(\"\\nüë• Mutual friends of Alice and Bob:\")\n",
    "for friend in mutual:\n",
    "    print(f\"   - {friend}\")"
   ]
