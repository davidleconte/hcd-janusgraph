  {
   "cell_type": "code",
   "execution_count": null,
   "id": "path-query",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Path from Alice to products through company\n",
    "query = \"\"\"\n",
    "g.V().has('person', 'name', 'Alice Johnson')\n",
