    "\n",
    "result = gc.submit(query).all().result()[0]\n",
    "df = pd.DataFrame(list(result.items()), columns=['Label', 'Count'])\n",
    "df = df.sort_values('Count', ascending=False)\n",
    "print(\"\\nüìä Vertex Distribution:\")\n",
    "display(df)"
