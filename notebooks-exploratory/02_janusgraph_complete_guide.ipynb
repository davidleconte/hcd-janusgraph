   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "friends-of-friends",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Friends of Alice's friends (2-hop)\n",
    "query = \"\"\"\n",
    "g.V().has('person', 'name', 'Alice Johnson').as('alice')\n",
    "  .out('knows').out('knows')\n",
