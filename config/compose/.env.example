# Environment variables for docker-compose
# Copy this file to .env and fill in your credentials before running.
# Generated: 2026-02-15

# OpenSearch
OPENSEARCH_INITIAL_ADMIN_PASSWORD=CHANGE_ME_TO_SECURE_PASSWORD_MIN_16_CHARS

# Grafana
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=CHANGE_ME_TO_SECURE_PASSWORD_MIN_16_CHARS

# API secure profile (required by docker-compose.prod-secure.override.yml)
API_JWT_SECRET=CHANGE_ME_TO_SECURE_JWT_SECRET_MIN_32_CHARS
API_USER_PASSWORD=CHANGE_ME_TO_SECURE_API_PASSWORD_MIN_16_CHARS

# SMTP (for AlertManager)
SMTP_PASSWORD=CHANGE_ME_TO_SECURE_PASSWORD_MIN_16_CHARS

# Slack (for AlertManager)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
